{
    "name": "fujagabriel/mobilenumbervalidator-cli",
    "description": "A sample mobile number validator using laravel cli",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Ajenifuja",
            "email": "gabrielfuja001@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "giggsey/libphonenumber-for-php": "^8.10",
        "laravel-zero/framework": "5.8.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename " 
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["mobilenumbervalidator-cli"]
}
